<div class="panel panel-default sessions">
	<div class="center">
	<font color="#34495e"><h3 class="animated bounceInDown">Welcome back!</h3></font>
	
	<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
	<div class="form-group animated fadeIn" >
	  	<%= f.email_field :email, autofocus: "true", class: "form-control animated fadeInLeft input_field", placeholder: "Email" %>

	    <%= f.password_field :password, autocomplete: "off", class: "form-control animated fadeInRight input_field", placeholder: "Password" %></div>

		<div class="session-base animated bounceInUp">

	  <!--
	  <% if devise_mapping.rememberable? -%>
	    
	    <div><%= f.check_box :remember_me %> <%= f.label "Keep me signed in" %></div>

	  <% end -%>
		-->
	
	  	<div><%= f.submit "Sign In", class: "btn btn-large sign-up" %></div>
			<font color="#bdc3c7"><%= render "devise/shared/links" %></font>
		</div><!-- .session-footer.animated.fadeIn -->
	
	<% end %>

	
</div><!-- .center -->
</div><!-- .panel.panel-default.sessions -->
